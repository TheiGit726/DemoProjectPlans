<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>DemoProjectPlans</display-name>
  <servlet>
  	<servlet-name>AddTask</servlet-name>
  	<servlet-class>com.projectplans.controller.AddTask</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddPrerequisite</servlet-name>
  	<servlet-class>com.projectplans.controller.AddPrerequisite</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>RunTask</servlet-name>
  	<servlet-class>com.projectplans.controller.RunTask</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AddTask</servlet-name>
  	<url-pattern>/AddTask</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddPrerequisite</servlet-name>
  	<url-pattern>/AddPrerequisite</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RunTask</servlet-name>
  	<url-pattern>/RunTask</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>addTask.jsp</welcome-file>
  </welcome-file-list>
</web-app>